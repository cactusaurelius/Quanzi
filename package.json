{
  "name": "quanzi",
  "version": "1.0.0",
  "description": "My Moleculer-based microservices project",
  "author": "A Shaaban",
  "engines": {
    "node": ">= 14.x.x"
  },
  "scripts": {
    "build": "tsc --build tsconfig.json",
    "dev": "ts-node ./node_modules/moleculer/bin/moleculer-runner.js --envfile ./deploy/.env --hot --repl --config moleculer.config.ts services/**/*.service.ts",
    "start": "moleculer-runner --config build/moleculer.config.js",
    "ci": "jest --watch",
    "test": "jest --coverage",
    "lint": "eslint --ext .js,.ts .",
    "lint:fix": "eslint --ext .js,.ts . --fix",
    "dc:up": "docker-compose up --build -d",
    "dc:logs": "docker-compose logs -f",
    "dc:down": "docker-compose down"
  },
  "keywords": [
    "microservices",
    "moleculer"
  ],
  "devDependencies": {
    "@types/node": "^13.13.40",
    "@types/nodemailer": "^6.4.1",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "jest": "^25.1.0",
    "jest-cli": "^25.1.0",
    "moleculer-decorators": "^1.3.0",
    "moleculer-repl": "^0.6.2",
    "ts-jest": "^25.3.0",
    "ts-node": "^8.8.1",
    "uuid": "^3.4.0"
  },
  "dependencies": {
    "@influxdata/influxdb-client": "^1.10.0",
    "@influxdata/influxdb-client-apis": "^1.11.0",
    "@types/jest": "^25.1.4",
    "@types/mkdirp": "^1.0.0",
    "ccxt": "^1.39.1",
    "i": "^0.3.6",
    "influx": "^5.7.0",
    "moleculer": "^0.14.0",
    "moment": "^2.22.2",
    "mongoose": "^5.11.18",
    "nats": "^1.4.12",
    "nodemailer": "^6.5.0",
    "typescript": "^3.8.3"
  }
}
